{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-sample-SnowLeopard.WebApi",
            "program": "${workspaceFolder}/sample/SnowLeopard.WebApi/bin/Debug/netcoreapp2.2/SnowLeopard.WebApi.dll",
            "args": [],
            "cwd": "${workspaceFolder}/sample/SnowLeopard.WebApi",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": true,
                "args": "${auto-detect-url}/swagger",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start ${auto-detect-url}/swagger"
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Launch (SnowLeopard.Console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-sample-SnowLeopard.Console",
            "program": "${workspaceFolder}/test/SnowLeopard.Console/bin/Debug/netcoreapp2.2/SnowLeopard.Console.dll",
            "args": [],
            "cwd": "${workspaceFolder}/test/SnowLeopard.Console",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Launch (SnowLeopard.Lynx.ConsoleTest)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-sample-SnowLeopard.Lynx.ConsoleTest",
            "program": "${workspaceFolder}/test/SnowLeopard.Lynx.ConsoleTest/bin/Debug/netcoreapp2.2/SnowLeopard.Lynx.ConsoleTest.dll",
            "args": [],
            "cwd": "${workspaceFolder}/test/SnowLeopard.Lynx.ConsoleTest",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Launch (SnowLeopard.Redis.ConsoleTest)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-sample-SnowLeopard.Redis.ConsoleTest",
            "program": "${workspaceFolder}/test/SnowLeopard.Redis.ConsoleTest/bin/Debug/netcoreapp2.2/SnowLeopard.Redis.ConsoleTest.dll",
            "args": [],
            "cwd": "${workspaceFolder}/test/SnowLeopard.Redis.ConsoleTest",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}